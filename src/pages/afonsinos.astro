---
import ContainedWithTitle from "@/layouts/ContainedWithTitle.astro";
import { afonsinoFilters, afonsinos } from "db";
import Afonsinos from "@/components/afonsinos/Afonsinos.svelte";

const imageFallback: string = "/assets/images/tuno_head.webp"

const imageTunos: string[] = Object.values(afonsinos).map((afonsino) => afonsino.imagem)

---

<ContainedWithTitle title="Afonsinos" preloadImage={[imageFallback]} prefetchImage={imageTunos}>

	<Afonsinos client:load afonsinos={afonsinos} afonsinoFilters={afonsinoFilters} imageFallback={imageFallback} />

</ContainedWithTitle>
